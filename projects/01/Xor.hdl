/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in = a, out = nota);
    Not(in = b, out = notb);

    And(a = a, b = notb, out = x);
    And(a = nota, b = b, out = y);

    Or(a = x, b = y, out = out);
}
